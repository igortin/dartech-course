{
  "configs":
  [
    {
      "interface": ":8080",
      "upstreams": [
        {
          "path": "/com",
          "method": "GET",
          "proxyMethod": "round-robin",
          "backends": [
            "https://google.com",
            "https://facebook.com"
          ]
        },
        {
          "path": "/ru",
          "method": "GET",
          "proxyMethod": "anycast",
          "backends": [
            "https://mail.ru",
            "https://lenta.ru"
          ]
        }
      ]
    },
    {
      "interface": ":8081",
      "upstreams": [
        {
          "path": "/movie",
          "method": "GET",
          "proxyMethod": "round-robin",
          "backends": [
            "https://ivi.ru",
            "https://megogo.ru"
          ]
        },
        {
          "path": "/clips",
          "method": "GET",
          "proxyMethod": "anycast",
          "backends": [
            "https://youtube.com",
            "https://kinobar.kz"
          ]
        }
      ]
    }
  ]
}